<div id = "proposal-index"></div>
<div class="row" id="proposal-index-banner">
</div>

<div class="container">

  <h1>Discover below all your proposals </h1>

  <% @proposals.each do |proposal| %>

  <h4 id="project-detail"><%= link_to proposal.project.title, project_path(proposal.project) %> </h4>

  <%= render "shared/proposal-card", proposal: proposal %>
  <% end %>

  <% if @proposals.nil? %>
  <%= "You don't have any proposals :(" %>
  <% end %>

</div>
</div>
